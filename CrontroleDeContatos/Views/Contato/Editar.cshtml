@{
    ViewData["Title"] = "Editar Contatos";
}
<div class="text-center">
    <h1 class="display-4">Editar Contatos</h1>

</div>
<form>
    <div class="form-group">
        <label for="nome">Nome</label>
        <input type="text" class="form-control" placeholder="Digite o nome do contato">
    </div>
    <div class="form-group">
        <label for="nome">Email</label>
        <input type="email" class="form-control" placeholder="Digite o email do contato">
    </div>
    <div class="form-group">
        <label for="nome">Numero</label>
        <input type="tel" class="form-control" placeholder="Digite o Numero do contato">
    </div>
    <br>
    <button type="submit" class="btn btn-primary btn-block">Editar</button>
    <a class=" btn btn-secondary btn-block" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
</form>
